{
    "interfaces": {
        "IUserProfileData": [
            "+getUserId(): int",
            "+getName(): string",
            "+getEmail(): string",
            "+getAge(): int",
            "+getHeight(): float",
            "+getWeight(): float"
        ],
        "IUserGoalsData": [
            "+getGoal(): string",
            "+getActivityLevel(): string",
            "+getDailyGoal(): float"
        ],
        "IMealData": [
            "+getMealId(): int",
            "+getName(): string",
            "+getTotalCalories(): float"
        ],
        "IMealPlanData": [
            "+getPlanId(): int",
            "+getUserId(): int",
            "+getMeals(): IMealData",
            "+getTotalCalories(): float",
            "+getDate(): date"
        ],
        "IFoodItemData": [
            "+getFoodId(): int",
            "+getName(): string",
            "+getCategory(): string",
            "+getCalories(): float"
        ],
        "IMealLogData": [
            "+getLogId(): int",
            "+getUserId(): int",
            "+getMealId(): int",
            "+getAddedCalories(): float",
            "+getDateTime(): datetime"
        ],
        "IProgressData": [
            "+getUserId(): int",
            "+getWeightHistory(): float",
            "+getCalorieHistory(): float"
        ],
        "IAuthenticationService": [
            "+login(email: string, password: string): IUserProfileData",
            "+register(profile: IUserProfileData)",
            "+logout(userId: int)"
        ],
        "IUserProfileService": [
            "+updateProfile(profile: IUserProfileData)",
            "+getProfile(userId: int): IUserProfileData"
        ],
        "IMealPlannerService": [
            "+generateMealPlan(userId: int): IMealPlanData",
            "+getMealPlan(userId: int, date: date): IMealPlanData",
            "+updateMealPlan(plan: IMealPlanData)"
        ],
        "IAIEngine": [
            "+analyzeMealImage(imgPath: string): IFoodItemData",
            "+estimateCalories(foodName: string): float"
        ],
        "IMealImageRecognizer": [
            "+recognizeImage(imgPath: string): IFoodItemData"
        ],
        "IFoodRepository": [
            "+addFood(item: IFoodItemData)",
            "+getFood(id: int): IFoodItemData",
            "+updateFood(item: IFoodItemData)",
            "+getFoods(): IFoodItemData",
            "+removeFood(id: int)"
        ],
        "IMealService": [
            "+createMeal(meal: IMealData)",
            "+getMeal(id: int): IMealData",
            "+updateMeal(meal: IMealData)",
            "+deleteMeal(id: int)"
        ],
        "IMealPlanService": [
            "+generateDailyPlan(userId: int, date: date): IMealPlanData",
            "+addMealToPlan(planId: int, mealId: int)",
            "+removeMealFromPlan(planId: int, mealId: int)"
        ],
        "IMealLogService": [
            "+addMealEntry(userId: int, mealId: int, dateTime: datetime)",
            "+removeMealEntry(logId: int)",
            "+getDailySummary(userId: int, date: date): IMealLogData"
        ],
        "IProgressService": [
            "+updateWeight(userId: int, weight: float)",
            "+updateCalories(userId: int, calories: float)",
            "+getProgress(userId: int): IProgressData"
        ],
        "IReportService": [
            "+generateReport(userId: int): string"
        ],
        "IReportExporter": [
            "+exportToCSV(data: string)",
            "+exportToPDF(data: string)"
        ]
    },
    "classes": {
        "UserProfile": {
            "implements": "IUserProfileData",
            "fields": [
                "-userId: int",
                "-name: string",
                "-email: string",
                "-age: int",
                "-height: float",
                "-weight: float"
            ]
        },
        "UserGoals": {
            "implements": "IUserGoalsData",
            "fields": [
                "-goal: string",
                "-activityLevel: string",
                "-dailyGoal: float"
            ]
        },
        "User": {
            "fields": [
                "-userId: int",
                "-profile: IUserProfileData",
                "-goals: IUserGoalsData",
                "-currentMealPlanId: int",
                "-progressId: int"
            ]
        },
        "Meal": {
            "implements": "IMealData",
            "fields": [
                "-mealId: int",
                "-name: string",
                "-totalCalories: float",
                "-carbs: float",
                "-protein: float",
                "-fats: float",
                "-ingredients: IFoodItemData"
            ]
        },
        "MealPlan": {
            "implements": "IMealPlanData",
            "fields": [
                "-planId: int",
                "-userId: int",
                "-meals: IMealData",
                "-totalCalories: float",
                "-date: date"
            ]
        },
        "FoodItem": {
            "implements": "IFoodItemData",
            "fields": [
                "-foodId: int",
                "-name: string",
                "-category: string",
                "-calories: float",
                "-protein: float",
                "-fats: float",
                "-carbs: float",
                "-tags: string"
            ]
        },
        "MealLog": {
            "implements": "IMealLogData",
            "fields": [
                "-logId: int",
                "-userId: int",
                "-mealId: int",
                "-addedCalories: float",
                "-dateTime: datetime"
            ]
        },
        "Progress": {
            "implements": "IProgressData",
            "fields": [
                "-userId: int",
                "-weightHistory: float",
                "-calorieHistory: float"
            ]
        },
        "AuthenticationService": {
            "implements": "IAuthenticationService",
            "fields": [
                "-serviceId: int",
                "-securityLevel: string",
                "-maxAttempts: int"
            ]
        },
        "UserProfileService": {
            "implements": "IUserProfileService",
            "fields": [
                "-foodRepo: IFoodRepository"
            ]
        },
        "MealPlannerService": {
            "implements": "IMealPlannerService",
            "fields": [
                "-aiEngine: IAIEngine",
                "-foodRepo: IFoodRepository"
            ]
        },
        "MealService": {
            "implements": "IMealService",
            "fields": [
                "-mealRepo: IMealData"
            ]
        },
        "MealPlanService": {
            "implements": "IMealPlanService",
            "fields": [
                "-planRepo: IMealPlanData"
            ]
        },
        "MealLogService": {
            "implements": "IMealLogService",
            "fields": [
                "-logRepo: IMealLogData",
                "-mealService: IMealService",
                "-userService: IUserProfileService"
            ]
        },
        "ProgressService": {
            "implements": "IProgressService",
            "fields": [
                "-progressRepo: IProgressData",
                "-reportService: IReportService",
                "-userService: IUserProfileService"
            ]
        },
        "ReportService": {
            "implements": "IReportService",
            "fields": [
                "-progressRepo: IProgressData"
            ]
        },
        "ReportExporter": {
            "implements": "IReportExporter",
            "fields": [
                "-exporterId: int",
                "-supportedFormats: string",
                "-defaultPath: string"
            ]
        },
        "FoodDatabase": {
            "implements": "IFoodRepository",
            "fields": [
                "-foods: IFoodItemData"
            ]
        },
        "AIEngine": {
            "implements": "IAIEngine",
            "fields": [
                "-model: string",
                "-foodRepository: IFoodRepository"
            ]
        },
        "MealImageRecognizer": {
            "implements": "IMealImageRecognizer",
            "fields": [
                "-modelPath: string"
            ]
        }
    },
    "relationships": [
        "User --> IUserProfileData : has",
        "User --> IUserGoalsData : has",
        "User --> IMealPlanData : references (currentMealPlanId)",
        "User --> IProgressData : references (progressId)",
        "AuthenticationService --> IUserProfileService : uses",
        "AuthenticationService --> IFoodRepository : may-check",
        "UserProfileService --> IFoodRepository : uses",
        "MealPlannerService --> IAIEngine : uses",
        "MealPlannerService --> IFoodRepository : uses",
        "MealPlannerService --> IMealPlanService : creates/updates plans",
        "AIEngine --> IFoodRepository : queries",
        "MealImageRecognizer --> IAIEngine : may delegate to",
        "MealService --> IFoodRepository : reads ingredients",
        "MealPlanService *-- IMealData : contains",
        "Meal --> IFoodItemData : contains",
        "MealLogService --> IMealLogData : manages",
        "MealLogService --> IMealService : uses",
        "MealLogService --> IUserProfileService : reads users",
        "ProgressService --> IProgressData : manages",
        "ProgressService --> IReportService : uses",
        "ProgressService --> IUserProfileService : reads users",
        "MealLog --> IMealData : logs",
        "MealLog --> IUserProfileData : belongs to",
        "Progress --> IReportService : provides data to",
        "ReportService --> IReportExporter : exports via"
    ]
}